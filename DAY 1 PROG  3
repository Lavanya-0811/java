//inheritance
public class rec
{
    int a = 3;
}
public class rit extends rec
{
    int b = 4;
    public static void main(String agrs[])
    {
        rit o = new rit();
        System.out.println(o.a+o.b);
    }
}
//ML1
public class one {
    int a =3;
}
public class two extend one
{
    int b = 4;
}
public class three extends tw0
{
    int c=5;
    public static void main(String args[])
    {
        three o = new three();
        System.out.println(o.a+o.b+o.c);
    }
}
